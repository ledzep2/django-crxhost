<?xml version='1.0' encoding='UTF-8'?>
<gupdate xmlns='http://www.google.com/update2/response' protocol='2.0'>
{% for crx in crxs %}
<app appid='{{crx.cid}}'>
<updatecheck codebase='{% url crx_download crx.crx.name crx.generated_version %}' version='{{crx.generated_version}}' />
</app>
{% endfor %}
</gupdate>